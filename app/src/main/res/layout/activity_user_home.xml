<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg2"
    tools:context=".User.UserHomeActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="81dp"
        android:layout_height="74dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/logo2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_aduan"
        android:layout_width="319dp"
        android:layout_height="78dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/shapelogin"
        android:drawableLeft="@drawable/search"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center"
        android:outlineAmbientShadowColor="@color/black"
        android:shadowColor="@android:color/transparent"
        android:text="cek laundry"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/btn_admin_antrian"
        android:layout_width="319dp"
        android:layout_height="78dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/shapelogin"
        android:drawableLeft="@drawable/logout"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center"
        android:outlineAmbientShadowColor="@color/black"
        android:shadowColor="@android:color/transparent"
        android:text="keluar"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_add" />

    <Button
        android:id="@+id/btn_add"
        android:layout_width="319dp"
        android:layout_height="78dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/shapelogin"
        android:drawableLeft="@drawable/cs"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center"
        android:outlineAmbientShadowColor="@color/black"
        android:shadowColor="@android:color/transparent"
        android:text="customer service"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_pesanan_baru" />

    <Button
        android:id="@+id/btn_pesanan_baru"
        android:layout_width="319dp"
        android:layout_height="78dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/shapelogin"
        android:drawableLeft="@drawable/cucian"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center"
        android:outlineAmbientShadowColor="@color/black"
        android:shadowColor="@android:color/transparent"
        android:text="pesan laundry"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_aduan" />


</androidx.constraintlayout.widget.ConstraintLayout>